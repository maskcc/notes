/**** -_- [BY TBC] merge  : /home/scmpf/compiler_src/zhaojinglun_989715_linux64/0/app/search/forum/fe/encourage//output/static/component/meizhi_vote/meizhi_vote.js -_- ****/
_.Module.define({
    path: 'encourage/component/meizhi_vote',
    requires: [
        'common/component/Card'
    ],
    sub: {
        _visit_card: null,
        _vote_card: null,
        _visit_card_ajax: null,
        _vote_button: null,
        _visit_card_option: {
            width: 280,
            height: 237
        },
        //
        _special_visit_card_option: {
            width: 280,
            height: 210
        },
        initial: function () {
            this._wrap = $('body');
            this._is_meizhi = (PageData.user.is_login && PageData.user.meizhi_level >= 0) ? true : false;
            this.bindEvents();
        },
        bindEvents: function () {
            var self = this;
            //
            this._wrap.delegate('.j_meizhi_vip', 'mouseenter', function () {
                self.buildVisitCard(this);
                return false;
            });
            this._wrap.delegate('.j_meizhi_vip', 'mouseleave', function() {
                self._visit_card_ajax && self._visit_card_ajax.abort();
                self._visit_card && self._visit_card.closeCard({
                    type: 'delayClose',
                    time: 200
                });
                self._visit_card = null;
                return false;
            });

            //
            this._wrap.delegate('.j_meizhi_vote', 'click', function (e) {
                if (self._vote_card) {
                    self._vote_card.closeCard();
                    self._vote_card = null;
                    if (self._vote_button == e.target) {	//
                        e.preventDefault();
                        return false;
                    }
                }
                self.buildVoteCard(this);
                self._vote_button = e.target;
                e.preventDefault();
            });
        },
        buildVoteCard: function (e) {
            var self = this;
            var data_field = $(e).getData();
            var user_id = data_field.user_id;
            $.ajax({
                type: 'post',
                url: '/encourage/get/meizhi/panel',
                data: {
                    user_id: user_id
                },
                dataType: 'json'
            }).success(function (json) {
                    if (json.no == 0) {
                        var data = json.data;
                        var html = self.buildVoteBtnGroup(data.can_vote, user_id, data.vote_count);
                        var j_target = $(e);
                        var left = j_target.offset().left - 280 / 2 + j_target.outerWidth(true) / 2;//
                        var top = j_target.offset().top - 64 - 12;//card

                        if (self._vote_card) {
                            self._vote_card.closeCard();
                            self._vote_card = null;
                        }
                        self._vote_card = self.use('common/component/Card', {
                            content: html,
                            card_css: {
                                top: top,
                                left: left,
                                width: 280,
                                height: 64
                            },
                            attr: "id='meizhi_vote_card'",
                            wrap: $('body')
                        });
                        self.bindVoteCardEvent();
                    }
                });
        },
        bindVoteCardEvent: function () {
            var self = this;
            self._vote_card.showCard();
            self._vote_card._j_card.delegate('.j_vote_button', 'click', function () {
                //
                $.stats.track('frs_btn', 'meizhi_vote');
                self.vote(this);
            });
        },
        vote: function (e) {
            var self = this;
            var j_button = $(e);
            var data_field = j_button.getData();
            var vote_type = data_field.vote_type;
            var checkUrl = '/encourage/post/meizhi/checkvcode';
            var vCodeUrl = '/encourage/post/meizhi/getvcode';
            _.Module.use('common/component/PostorServiceMeizhi', [], function (postInstance) {
                postInstance.postHandler({
                    data: {
                        content: "",
                        tbs: PageData.tbs,
                        fid: PageData.forum.id || PageData.forum.forum_id,
                        kw: PageData.forum.name,
                        uid: data_field.user_id,
                        scid: PageData.user.id || 0,
                        vtype: vote_type,
                        ie: "utf-8",
                        vcode:""
                    },
                    checkUrl: checkUrl,
                    vCodeUrl: vCodeUrl,
                    url: '/encourage/post/meizhi/vote',
                    type: "vote",
                    success: function (res) {
                        var data = res;
                        var j_add_one = $('<img class="add_one_icon" src="/tb/static-encourage/img/meizhi/add_one.gif">');
                        j_add_one.appendTo(j_button).animate({top: "-=20", opacity: 0}, 1000);
                        var vote_count = self.cutVoteCount(data.vote_count[vote_type]);
                        vote_count = vote_count ? vote_count : 1;
                        j_button.find('.j_vote_count').text(vote_count);
                        var j_button_wrap = j_button.parent();
                        j_button_wrap.find('.j_vote_text').addClass('vote_text_small');
                        j_button_wrap.find('.j_vote_count').removeClass('hide');
                        j_button_wrap.siblings('.j_vote_tip').text('');
                        j_button_wrap.addClass('disabled').find('.j_vote_button').removeClass('j_vote_button');
                        if(data.hasCheckVcode == 1){
                            self._msgDialog();
                        }
                        //
                        $.stats.track('vote_succ', 'meizhi_vote');
                    }
                });
            });
        },
        _msgDialog:function(){
            var dialogOp = {
                image : '<img style="border:none;" src="http://static.tieba.baidu.com/tb/img/messageFace.gif">',
                subMessage : '',
                message : '',
                dialogSet : {
                    width:410,
                    height:50,
                    title: ''
                }
            };
            var _html = '<div style="margin: 0px 30px 0px 30px;font-size:14px;line-height:20px;text-align:left;font-family: \"\";"><table style="width:90%"><tbody><tr><td width="60">'+dialogOp.image+'</td><td valign="middle" style="font-size:20px;line-height:22px;font-family: \"\";">'+dialogOp.subMessage+'</td></tr></tbody></table>'+dialogOp.message+'</div>';
            $.dialog.alert(_html,dialogOp.dialogSet);
        },
        buildVisitCard: function (e) {
            var self = this;
            var data_field = $(e).parent().getData();
            var user_id = data_field.user_id;
            //
            if (this._visit_card_ajax) {
                this._visit_card_ajax.abort();
            }
            this._visit_card_ajax = $.ajax({
                type: 'post',
                url: '/encourage/get/meizhi/panel',
                data: {
                    user_id:user_id,
                    type: 1
                },
                dataType: 'json'
            }).success(function (json) {
                    if (json.no == 0) {
                        var data = json.data;
                        var level = (data.level - 1) ? (data.level - 1) : '';
                        var percent_arr = self.calPercent(data.vote_count);
                        var user_link = '/i/sys/jump?un=' + encodeURIComponent(data.uname) + '&ie=utf-8';
                        var html = [
                            '<a href=' + user_link + ' title="" class="meizhi_avatar" target="_blank"><img src="' + data.avatar + '"></a>',
                            '<div class="meizhi_corner"></div>',
                            '<div class="meizhi_info">',
                            '<h1><a class="name" href=' + user_link + ' title="" target="_blank">' + data.uname + '</a></h1>',
                            '<div><span class="level">MVP' + level + '</span></div>',
                            '<div><span class="forum_name">' + data.forum_name + '</span></div>',
                            '</div>',
                            '<p><a href = "/p/' + data.thread_id + '" target = "_blank" >Ta>>></a></p>',
                            '<p></p>',
                            '<div class="meizhi_vote_weight_wrap">',
                            '<span class="vote_weight_meizhi" style="width:' + percent_arr.meizhi_orign_percent + '%">' + percent_arr.meizhi_need_percent + '</span>',
                            '<span class="vote_weight_weiniang" style="width:' + percent_arr.weiniang_orign_percent + '%;">' + percent_arr.weiniang_need_percent + '</span>',
                            '<span class="vote_weight_renyao" style="width:' + percent_arr.renyao_orign_percent + '%;">' + percent_arr.renyao_need_percent + '</span>',
                            '</div>'
                        ].join('');
                        var is_meizhi = self._is_meizhi;
                        if (!is_meizhi) {
                            if (data.can_vote) {
                                html += '<p class="j_vote_tip"></p>';
                            }
                            else {
                                html += '<p></p>';
                            }
                        }
                        html += self.buildVoteBtnGroup(data.can_vote, user_id, data.vote_count);
                        html += self.addTip(data, is_meizhi);
                        
                        var j_target = $(e);
                        var op = is_meizhi ? self._special_visit_card_option : self._visit_card_option;
                        if (self._visit_card) {
                            self._visit_card.closeCard();
                            self._visit_card = null;
                        }
                        self._visit_card = self.use('common/component/Card', {
                            content: html,
                            card_css: {
                                width: op.width,
                                height: op.height
                            },
                            auto_positon: true,
                            event_target: j_target,
                            attr: "id='meizhi_visit_card'",
                            wrap: $('body')
                        });
                        self.bindVisitCardEvent();
                    }
                });
        },
        addTip: function(data, is_meizhi) {
            var temp;
            if (is_meizhi) {
                temp = '<p class="level_up_tip"><span class="level_up_tip_title">:</span>' + data.exp_value + '' + data.levelup_left + '</p>';
            }
            else {
                temp = '<a class="meizhi_card_tip" href="http://tieba.baidu.com/tb/zt/meizhi/index.html" target="_blank">>></a>';
            }
            return temp;
        },
		//orign_percentneed_percent
		calPercent : function(vote_arr) {
			var vote_total = vote_arr.meizhi + vote_arr.weiniang + vote_arr.renyao;
			var meizhi_orign_percent = Math.round((vote_arr.meizhi / vote_total * 100));
			var weiniang_orign_percent = Math.round((vote_arr.weiniang / vote_total * 100));
			var renyao_orign_percent = 100 - meizhi_orign_percent - weiniang_orign_percent;
			var min_percent = 25 / 250 * 100; //
			var meizhi_need_percent = meizhi_orign_percent > min_percent ? (meizhi_orign_percent+'%') : '';
			var weiniang_need_percent = weiniang_orign_percent > min_percent ? (weiniang_orign_percent+'%') : '';
			var renyao_need_percent = renyao_orign_percent > min_percent ? (renyao_orign_percent+'%') : '';
            //ie7
            if ($.browser.msie && $.browser.version < 8) {
                if (meizhi_orign_percent > 30) {
                    meizhi_orign_percent -= 1;
                }else if (weiniang_orign_percent > 30) {
                    weiniang_orign_percent -= 1;
                }else if (renyao_orign_percent > 30) {
                    renyao_orign_percent -= 1;
                }
            }
			var result = {
				meizhi_orign_percent : meizhi_orign_percent,
				weiniang_orign_percent : weiniang_orign_percent,
				renyao_orign_percent : renyao_orign_percent,
				meizhi_need_percent : meizhi_need_percent,
				weiniang_need_percent : weiniang_need_percent,
				renyao_need_percent : renyao_need_percent
			};
			return result;
		},
		buildVoteBtnGroup: function(can_vote, user_id, vote_count_arr) {
			var temp;
            if (this._is_meizhi) {
                temp = [
                    '<div class="meizhi_vote_button_wrap meizhi_disabled">',
                        '<div class="vote_button vote_button_meizhi">',
                            '<div class="vote_text vote_text_small"></div>',
                            '<div class="vote_count">' + this.cutVoteCount(vote_count_arr.meizhi) + '</div>',
                        '</div>',
                        '<span class="btn_split"></span>',
                        '<div class="vote_button vote_button_weiniang">',
                            '<div class="vote_text vote_text_small"></div>',
                            '<div class="vote_count">' + this.cutVoteCount(vote_count_arr.weiniang) + '</div>',
                        '</div>',
                        '<span class="btn_split"></span>',
                        '<div class="vote_button vote_button_renyao">',
                            '<div class="vote_text vote_text_small"></div>',
                            '<div class="vote_count">' + this.cutVoteCount(vote_count_arr.renyao) + '</div>',
                        '</div>',
                    '</div>'
                ].join('');
            }
            else {
                if (can_vote) {
                    var data_field1 = {user_id: user_id, vote_type: 'meizhi'};
                    var data_field2 = {user_id: user_id, vote_type: 'weiniang'};
                    var data_field3 = {user_id: user_id, vote_type: 'renyao'};
                    temp = [
                        '<div class="meizhi_vote_button_wrap">',
                            '<div class="j_vote_button vote_button vote_button_meizhi"' + $.tb.dataField(data_field1) + '>',
                                '<div class="vote_text j_vote_text"></div>',
                                '<div class="vote_count j_vote_count hide">' + this.cutVoteCount(vote_count_arr.meizhi) + '</div>',
                            '</div>',
                            '<div class="j_vote_button vote_button vote_button_weiniang"' + $.tb.dataField(data_field2) + '>',
                                '<div class="vote_text j_vote_text"></div>',
                                '<div class="vote_count j_vote_count hide">' + this.cutVoteCount(vote_count_arr.weiniang) + '</div>',
                            '</div>',
                            '<div class="j_vote_button vote_button vote_button_renyao"' + $.tb.dataField(data_field3) + '>',
                                '<div class="vote_text j_vote_text"></div>',
                                '<div class="vote_count j_vote_count hide">' + this.cutVoteCount(vote_count_arr.renyao) + '</div>',
                            '</div>',
                        '</div>'
                    ].join('');
                }
                else {
                    temp = [
                        '<div class="meizhi_vote_button_wrap disabled">',
                            '<div class="vote_button vote_button_meizhi">',
                                '<div class="vote_text vote_text_small"></div>',
                                '<div class="vote_count">' + this.cutVoteCount(vote_count_arr.meizhi) + '</div>',
                            '</div>',
                            '<div class="vote_button vote_button_weiniang">',
                                '<div class="vote_text vote_text_small"></div>',
                                '<div class="vote_count">' + this.cutVoteCount(vote_count_arr.weiniang) + '</div>',
                            '</div>',
                            '<div class="vote_button vote_button_renyao">',
                                '<div class="vote_text vote_text_small"></div>',
                                '<div class="vote_count">' + this.cutVoteCount(vote_count_arr.renyao) + '</div>',
                            '</div>',
                        '</div>'
                    ].join('');
                }
            }
			return temp;
		},
		cutVoteCount : function(count) {
			return count > 9999 ? '9999+' : count;
		},
		bindVisitCardEvent: function() {
			var self = this;
			this._visit_card.showCard({
				type: 'delayShow',
				time: 200
			});			
			this._visit_card._j_card.delegate('.j_vote_button', 'click', function() {
                //
                $.stats.track('card_btn', 'meizhi_vote');
				self.vote(this);
			});
		}
	}
});

